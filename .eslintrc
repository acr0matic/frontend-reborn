{
  "extends": "standard",
  "ignorePatterns": [
    "src/js/libs/**/*.js"
  ],
  "rules": {
    "no-new": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "eol-last": "off",
    "semi": [
      "error",
      "always"
    ],
    "space-before-function-paren": "off",
    "brace-style": 0
  }
}